{
    "name": "sgm-web-api",
    "version": "1.0.0",
    "private": true,
    "description": "static server and main API for web version of sgm app",
    "license": "UNLICENSED",
    "author": "Nikita Demin",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "npm run build:be && npm run build:fe && echo 'copy fe stuff to public'",
        "build:be": "nest build backend --path ./tsconfig.backend.json",
        "build:fe": "node ./src/frontend/scripts/build.js",
        "doc:swagger": "ts-node ./docs/swagger.ts",
        "lint": "npm run lin:fe && npm run lint:be",
        "lint:be": "npx eslint ./src/main.ts ./src/app.module.ts ./src/shared ./src/api --config .eslintrc.js --fix",
        "lint:fe": "npx eslint ./src/frontend/src --config ./src/frontend/.eslintrc.json --ext .ts,.tsx --fix",
        "start": "node dist/main.js",
        "start:be": "nest start --watch backend --path ./tsconfig.backend.json",
        "start:be:debug": "nest start --debug --watch backend --path ./tsconfig.backend.json",
        "start:fe": "node ./src/frontend/scripts/start.js",
        "test": "npm run test:be && npm run test:fe",
        "test:be": "jest --config ./jest.config.backend.json",
        "test:be:cov": "jest --config ./jest.config.backend.json --coverage",
        "test:be:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./jest.config.backend.json --runInBand",
        "test:be:e2e": "jest --config ./test/jest-e2e.json",
        "test:fe": "node ./src/frontend/scripts/test.js --config ./jest.config.frontend.json"
    },
